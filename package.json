    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-vue": "^5.1.6",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
